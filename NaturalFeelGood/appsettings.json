{
  "DynamoDbSettings": {
    "ServiceURL": "http://localhost:8000", // DynamoDB endpoint URL (local or AWS)
    "AccessKey": "your-access-key",        // AWS access key for DynamoDB
    "SecretKey": "your-secret-key",        // AWS secret key for DynamoDB
    "Region": "us-east-1",                 // AWS region for DynamoDB
    "TablePrefix": "NaturalFeelGood"       // Prefix for DynamoDB tables
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
